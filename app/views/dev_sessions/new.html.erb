<h1> Developer Page </h1>

<%= form_tag dev_sessions_path do %>
  <p>
    <%= label_tag :email %><br>
    <%= text_field_tag :email, params[:email] %>
  </p>
  <p>
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
  </p>
  <p class="button">
    <%= submit_tag "authenticate" %>
  </p>
<% end %>